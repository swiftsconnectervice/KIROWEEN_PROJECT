generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL") 
}

model Claim {
  id            String   @id
  policyNumber  String
  claimantName  String
  location      String
  damageType    String
  amount        Float
  dateOfLoss    DateTime
  status        String
  decision      String?
  fraudRisk     String?
  aiReasoning   String?
  weatherEvent  String?
  createdAt     DateTime @default(now())
}